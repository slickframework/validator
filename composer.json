{
  "name": "slick/validator",
  "description": "Slick/Validator is a simple library with useful input validators",
  "license": "MIT",
  "homepage": "https://github.com/slickframework/validator",
  "keywords": [
    "slick",
    "validation",
    "input validation"
  ],
  "version": "v1.1.0",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Slick\\Validator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slick\\Tests\\Validator\\": "tests"
    }
  },
  "require": {
    "slick/common": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1",
    "behat/behat": "~3.0.4"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev",
      "dev-develop": "1.2-dev"
    }
  }
}
